t clr
: clr
${ac_dA}TPARM_DECLARED${ac_dB}TPARM_DECLARED${ac_dC}1${ac_dD}
${ac_dA}HAVE_TPARM${ac_dB}HAVE_TPARM${ac_dC}1${ac_dD}
${ac_dA}HAVE_NCURSES_H${ac_dB}HAVE_NCURSES_H${ac_dC}1${ac_dD}
