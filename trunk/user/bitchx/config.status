${ac_dA}UNIX_MAIL${ac_dB}UNIX_MAIL${ac_dC}"/var/spool/mail"${ac_dD}
${ac_dA}DEFAULT_CTOOLZ_DIR${ac_dB}DEFAULT_CTOOLZ_DIR${ac_dC}"~/.BitchX"${ac_dD}
${ac_dA}DEFAULT_MSGLOGFILE${ac_dB}DEFAULT_MSGLOGFILE${ac_dC}"BitchX.away"${ac_dD}
${ac_dA}DEFAULT_BITCHX_HELP_FILE${ac_dB}DEFAULT_BITCHX_HELP_FILE${ac_dC}"BitchX.help"${ac_dD}
${ac_dA}DEFAULT_SCRIPT_HELP_FILE${ac_dB}DEFAULT_SCRIPT_HELP_FILE${ac_dC}"BitchX.help"${ac_dD}
${ac_dA}DEFAULT_BITCHX_KICK_FILE${ac_dB}DEFAULT_BITCHX_KICK_FILE${ac_dC}"BitchX.kick"${ac_dD}
${ac_dA}DEFAULT_BITCHX_QUIT_FILE${ac_dB}DEFAULT_BITCHX_QUIT_FILE${ac_dC}"BitchX.quit"${ac_dD}
${ac_dA}DEFAULT_BITCHX_IRCNAME_FILE${ac_dB}DEFAULT_BITCHX_IRCNAME_FILE${ac_dC}"BitchX.ircnames"${ac_dD}
CEOF
  sed -f $tmp/defines.sed $tmp/in >$tmp/out
  rm -f $tmp/in
  mv $tmp/out $tmp/in

  fi # egrep

  # Handle all the #undef templates
  cat >$tmp/undefs.sed <<CEOF
/^[ 	]*#[ 	]*undef/!b
t clr
: clr
${ac_uA}PACKAGE_NAME${ac_uB}PACKAGE_NAME${ac_uC}""${ac_uD}
${ac_uA}PACKAGE_TARNAME${ac_uB}PACKAGE_TARNAME${ac_uC}""${ac_uD}
