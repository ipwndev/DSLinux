#!/bin/sh

mount -t proc none /proc
mount -t vfat -o noatime /dev/hda1 /m3
# some SD cards to not have a partition table
# try mounting /dev/hda if mounting /dev/hda1 fails
if [ "$?" != "0" ]
then
	mount -t vfat -o noatime /dev/hda /m3
	[ "$?" = "0" ] || echo "Sorry, failed to mount your SD card."
fi

. /etc/rc.common
