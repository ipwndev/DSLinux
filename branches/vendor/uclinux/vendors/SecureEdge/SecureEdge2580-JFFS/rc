hostname SecureEdge 
date `cat /etc/build-date`
mount -t proc proc /proc
mount -n -o remount,rw /
rm -rf /tmp/*
rm -rf /var/tmp/*
rm -f /var/run/*
rm -f /var/log/*.pid
rm -f /var/lock/*
rm -f /var/empty/*
/bin/flatfsd -r
insmod ipsec
insmod hifn7951
/bin/firewall -i
/bin/sh /etc/config/start
/bin/firewall &
echo "/etc/rc complete."
