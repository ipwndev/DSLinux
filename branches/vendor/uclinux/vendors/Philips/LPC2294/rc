hostname Philips-LPC22xx 
/bin/expand /etc/ramfs.img /dev/ram1
mount -t proc proc /proc
mount -t ext2 /dev/ram1 /var
mkdir /var/tmp
mkdir /var/log
mkdir /var/run
mkdir /var/lock
mkdir /var/empty
lcd_wlsi &
ifconfig lo 127.0.0.1
ifconfig eth0 192.168.0.220 broadcast 192.168.0.255 netmask 255.255.255.0
httpd &
cat /etc/motd
sh
