#!/bin/sh
echo
echo Generating Unicode upcase table.
(
echo '/* Automatically generated by gen-upc */'
echo
echo 'struct { unsigned lo; unsigned up; } unicode_upcase [] = {'
(echo '  160  32';cat upcase.uni) | sed 's/^[ 	]*//' | sed 's/[ 	]*$//' | sed 's/  */,	/' | sort -n | sed 's/^/	{ /' | sed 's/$/ },/'
echo '};'
) > ../upcase.inc
echo Done.
echo
