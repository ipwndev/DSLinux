config TARGET_ARCH
	default "nios2"

config HAVE_ELF
	bool
	select ARCH_HAS_NO_MMU
	select ARCH_LITTLE_ENDIAN
	select HAVE_NO_PIC
	select HAVE_NO_SHARED
	select ARCH_HAS_NO_LDSO
	default y

config ARCH_CFLAGS
	string

config ARCH_LDFLAGS
	string

config LIBGCC_CFLAGS
	string

config CROSS
    string
	default nios2-elf-

