# Server-side
<reply>
<data>
data blobb
</data>
</datacheck>
</reply>

# Client-side
<client>
<features>
ipv6
</features>
<server>
ftp
</server>
 <name>
Get two FTP files with no remote EPRT or LPRT support
 </name>
 <command>
ftp://%HOSTIP:%FTPPORT/a/path/212 ftp://%HOSTIP:%FTPPORT/a/path/212 -P -
</command>
<file name="log/ftpserver.cmd">
REPLY EPRT 500 no such command
REPLY LPRT 500 no such command
</file>
<stdout>
data blobb
data blobb
</stdout>
</client>

# Verify data after the test has been "shot"
<verify>
<strippart>
s/^(EPRT \|1\|127.0.0.1).*/$1/
s/^(LPRT 4,4,127,0,0,1,).*/$1/
s/^(PORT 127,0,0,1,).*/$1/
</strippart>
<protocol>
USER anonymous
PASS curl_by_daniel@haxx.se
PWD
CWD a
CWD path
EPRT |1|127.0.0.1
LPRT 4,4,127,0,0,1,
PORT 127,0,0,1,
TYPE I
SIZE 212
RETR 212
PORT 127,0,0,1,
TYPE I
SIZE 212
RETR 212
QUIT
</protocol>
</verify>
