	rm -f subst.sed subst.rules
	rm -f test.out
	rm -f `cat substfiles substfiles2 |sed '/^#/d'`
	rm -f GNUmakefile
	rm -f a.out conftest.c include/bsd-games.h
	rm -f exec.libs.tmp exec.objs.tmp

