# KDE Config file
[general]
i18ndir=/usr/share/i18n/charmaps

[aliases]
iso-ir-111=koi8-r
koi8-ru=koi8-r
koi unified=koi8-r
windows-1250=cp1250
cp-1250=cp1250
x-cp-1250=cp1250
windows-1251=cp1251
x-windows-1251=cp1251
cp-1251=cp1251
x-cp-1251=cp1251
utf-8=unicode-1-1-utf-8
#utf-7=unicode-1-1-utf-7
x-utf-8=unicode-1-1-utf-8
#x-utf-7=unicode-1-1-utf-7

[conversionHints]
cp1250=iso-8859-2,iso-8859-1
koi8-r=iso-8859-5
cp852=iso-8859-2,iso-8859-1
#unicode-1-1-utf-7=iso-8859-1
unicode-1-1-utf-8=iso-8859-1

[faces]
adobe-symbol=symbol
koi8-r=*_koi8

[XNames]
us-ascii=iso8859-1
