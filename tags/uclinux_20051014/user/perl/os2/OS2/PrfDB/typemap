BOOL			T_IV
ULONG			T_IV
HINI			T_IV
HAB			T_IV
PSZ			T_PVNULL

#############################################################################
INPUT
T_PVNULL
	$var = ( SvOK($arg) ? ($type)SvPV($arg,PL_na) : NULL )
#############################################################################
OUTPUT
T_PVNULL
	sv_setpv((SV*)$arg, $var);
