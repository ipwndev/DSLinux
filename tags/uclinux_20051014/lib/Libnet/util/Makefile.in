all: util

util:
	if ! test -e ../lib/libnet.a; then \
	    cd .. &&  make; \
	fi
	cd Get-mac; make all

clean:
	cd Get-mac; make clean

distclean: clean
	cd Get-mac; make distclean
	rm -f Makefile

