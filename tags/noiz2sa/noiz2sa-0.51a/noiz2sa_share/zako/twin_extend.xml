<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="topmove">
<changeDirection>
<direction type="absolute">120+$rand*120</direction>
<term>90</term>
</changeDirection>
<changeSpeed>
<speed>0.8</speed>
<term>1</term>
</changeSpeed>
<wait>1</wait>
</action>

<action label="topshot">
<fire>
<bulletRef label="seed"/>
</fire>
</action>

<bullet label="seed">
<speed>0</speed>
<action>
<fire>
<direction type="relative">90</direction>
<bulletRef label="seed2"/>
</fire>
<fire>
<direction type="relative">-90</direction>
<bulletRef label="seed2"/>
</fire>
<vanish/>
</action>
</bullet>

<bullet label="seed2">
<speed>2</speed>
<action>
<wait>1</wait>
<fire>
<speed>0.5</speed>
<bullet>
<action>
<repeat>
<times>2+$rank*4</times>
<action>
<fire>
<speed type="sequence">0.1</speed>
<bullet/>
</fire>
</action>
</repeat>
<vanish/>
</action>
</bullet>
</fire>
<vanish/>
</action>
</bullet>

</bulletml>
