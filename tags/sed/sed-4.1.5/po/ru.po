msgid "error in subprocess"
msgstr "ошибка в подпроцессе"

#: sed/execute.c:1208
msgid "option `e' not supported"
msgstr "опция `e' не поддерживается"

#: sed/execute.c:1389
msgid "`e' command not supported"
msgstr "команда `e' не поддерживается"

